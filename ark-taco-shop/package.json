        "joi": "^14.3.1",
        "vision": "^5.4.4"
    },
    "devDependencies": {
        "@sindresorhus/tsconfig": "^0.2.1",
        "@types/node": "^11.10.5",
        "cross-env": "^5.2.0",
        "jest": "^23.1.0",
        "jest-extended": "^0.7.2",
        "prettier": "^1.16.4",
